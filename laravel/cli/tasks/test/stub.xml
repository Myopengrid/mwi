<phpunit colors="true"
         bootstrap="{{bootstrap}}"
         backupGlobals="false">
	<testsuites>
		<testsuite name="Test Suite">
			<directory suffix=".test.php">{{directory}}</directory>
		</testsuite>
	</testsuites>
	<logging>
      		<log type="coverage-html" target="/tmp/report" charset="UTF-8" highlight="false" lowUpperBound="35" highLowerBound="70"/>
	</logging>
</phpunit>
